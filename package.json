{
  "name": "svelte-element-ui",
  "version": "1.0.11",
  "description": "svelte-element-ui is a element-ui component library base on Svelte.",
  "scripts": {
    "build": "rollup -c",
    "test:watch": "npm run test -- --watch",
    "test": "jest __test__",
    "demo-dev": "rollup -c demo/rollup.config.js -w",
    "demo-start": "sirv demo/public",
    "demo-build": "rollup -c demo/rollup.config.js",
    "validate": "svelte-check",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -o docs",
    "c": "git-cz",
    "release": "standard-version",
    "prepublishOnly": "npm run test && npm run build",
    "clear": "rimraf node_modules"
  },
  "files": [
    "lib/**/*"
  ],
  "main": "lib/seu.es.js",
  "typings": "types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/koory1st/svelte-element-ui.git"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-typescript": "^4.0.0",
    "@storybook/addon-actions": "^6.0.21",
    "@storybook/addon-knobs": "^6.0.19",
    "@storybook/addons": "^6.0.19",
    "@storybook/svelte": "^6.0.19",
    "@testing-library/jest-dom": "^5.11.7",
    "@testing-library/svelte": "^3.0.3",
    "@tsconfig/svelte": "^1.0.0",
    "array2string": "^1.1.1",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.1.0",
    "commitizen": "^4.2.2",
    "cz-conventional-changelog": "^3.3.0",
    "jest": "^26.6.3",
    "node-sass": "^5.0.0",
    "path": "^0.12.7",
    "rollup": "^2.26.11",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-livereload": "^1.0.0",
    "rollup-plugin-postcss": "^3.1.8",
    "rollup-plugin-scss": "^2.6.0",
    "rollup-plugin-svelte": "^6.1.1",
    "rollup-plugin-terser": "^6.0.0",
    "sirv-cli": "^1.0.0",
    "standard-version": "^9.0.0",
    "svelte": "^3.31.0",
    "svelte-check": "^1.0.41",
    "svelte-jester": "^1.2.0",
    "svelte-loader": "^2.13.6",
    "svelte-preprocess": "^4.6.1",
    "ts-jest": "^26.4.3",
    "tslib": "^2.0.0",
    "typescript": "^3.9.7"
  },
  "standard-version": {},
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.svelte$": [
        "svelte-jester",
        {
          "preprocess": true,
          "debug": false,
          "compilerOptions": {
            "dev": false
          }
        }
      ],
      "^.+\\.ts$": "ts-jest"
    },
    "moduleFileExtensions": [
      "js",
      "ts",
      "svelte"
    ],
    "globals": {
      "ts-jest": {
        "tsconfig": "__test__/tsconfig.test.json"
      }
    }
  },
  "setupFilesAfterEnv": [
    "@testing-library/jest-dom/extend-expect"
  ],
  "keywords": [
    "UI",
    "Svelte",
    "UI-Library",
    "element"
  ],
  "author": "koory1st",
  "license": "MIT",
  "homepage": "https://github.com/koory1st/svelte-element-ui"
}
