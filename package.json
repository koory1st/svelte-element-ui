{
  "name": "svelte-element-ui",
  "version": "1.1.8",
  "description": "svelte-element-ui is a element-ui component library base on Svelte.",
  "scripts": {
    "dev": "rollup -c -w",
    "start": "sirv public",
    "build": "rollup -c",
    "test:watch": "npm run test -- --watch",
    "test": "jest __test__",
    "validate": "svelte-check",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -o docs",
    "c": "git-cz",
    "release": "standard-version",
    "prepublishOnly": "npm run test && npm run build",
    "clear": "rimraf node_modules"
  },
  "module": "dist/index.min.mjs",
  "main": "dist/index.min.js",
  "typings": "types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/koory1st/svelte-element-ui.git"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@popperjs/core": "^2.6.0",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "@rollup/plugin-typescript": "^4.1.2",
    "@storybook/addon-actions": "^6.1.16",
    "@storybook/addon-knobs": "^6.1.16",
    "@storybook/addons": "^6.1.16",
    "@storybook/svelte": "^6.1.16",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/svelte": "^3.0.3",
    "@tsconfig/svelte": "^1.0.10",
    "array2string": "^1.2.4",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "commitizen": "^4.2.3",
    "cz-conventional-changelog": "^3.3.0",
    "jest": "^26.6.3",
    "node-sass": "^5.0.0",
    "path": "^0.12.7",
    "rollup": "^2.38.4",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-livereload": "^1.3.0",
    "rollup-plugin-postcss": "^3.1.8",
    "rollup-plugin-scss": "^2.6.1",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^6.1.0",
    "sirv-cli": "^1.0.11",
    "standard-version": "^9.1.0",
    "svelte": "^3.32.1",
    "svelte-check": "^1.1.32",
    "svelte-collapse": "^0.0.2",
    "svelte-jester": "^1.2.0",
    "svelte-loader": "^2.13.6",
    "svelte-popperjs": "^1.2.1",
    "svelte-portal": "^2.1.1",
    "svelte-preprocess": "^4.6.5",
    "ts-jest": "^26.5.0",
    "tslib": "^2.1.0",
    "typescript": "^3.9.7"
  },
  "standard-version": {},
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.svelte$": [
        "svelte-jester",
        {
          "preprocess": true,
          "debug": false,
          "compilerOptions": {
            "dev": false
          }
        }
      ],
      "^.+\\.ts$": "ts-jest"
    },
    "moduleFileExtensions": [
      "js",
      "ts",
      "svelte"
    ],
    "globals": {
      "ts-jest": {
        "tsconfig": "__test__/tsconfig.test.json"
      }
    }
  },
  "setupFilesAfterEnv": [
    "@testing-library/jest-dom/extend-expect"
  ],
  "keywords": [
    "UI",
    "Svelte",
    "UI-Library",
    "element"
  ],
  "author": "koory1st",
  "license": "MIT",
  "homepage": "https://github.com/koory1st/svelte-element-ui"
}
