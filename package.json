{
  "name": "svelte-element-ui",
  "version": "1.3.4",
  "description": "svelte-element-ui is a element-ui component library base on Svelte.",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "start": "vite preview",
    "test:watch": "npm run test -- --watch",
    "test": "jest __test__",
    "validate": "svelte-check",
    "c": "git-cz",
    "release": "standard-version",
    "prepublishOnly": "npm run test && npm run build",
    "clear": "rimraf node_modules",
    "build-site": "cd site && npm run build"
  },
  "files": [
    "src/index.ts",
    "packages",
    "static",
    "dist"
  ],
  "svelte": "src/index.ts",
  "main": "dist/svelte-element-ui.umd.js",
  "module": "dist/svelte-element-ui.es.js",
  "typings": "types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/koory1st/svelte-element-ui.git"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/preset-env": "^7.13.12",
    "@popperjs/core": "^2.9.1",
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.5",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/svelte": "^3.0.3",
    "@tsconfig/svelte": "^1.0.10",
    "array2string": "^1.2.4",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "commitizen": "^4.2.3",
    "cz-conventional-changelog": "^3.3.0",
    "jest": "^26.6.3",
    "sass": "^1.32.8",
    "standard-version": "^9.1.1",
    "svelte": "^3.35.0",
    "svelte-check": "^1.3.0",
    "svelte-collapse": "^0.0.2",
    "svelte-jester": "^1.3.2",
    "svelte-popperjs": "^1.2.3",
    "svelte-portal": "^2.1.1",
    "svelte-preprocess": "^4.7.0",
    "ts-jest": "^26.5.4",
    "tslib": "^2.1.0",
    "typescript": "^4.2.4",
    "vite": "^2.1.3"
  },
  "standard-version": {},
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.svelte$": [
        "svelte-jester",
        {
          "preprocess": true,
          "debug": false,
          "compilerOptions": {
            "dev": false
          }
        }
      ],
      "^.+\\.ts$": "ts-jest"
    },
    "moduleFileExtensions": [
      "js",
      "ts",
      "svelte"
    ],
    "globals": {
      "ts-jest": {
        "tsconfig": "__test__/tsconfig.test.json"
      }
    }
  },
  "setupFilesAfterEnv": [
    "@testing-library/jest-dom/extend-expect"
  ],
  "keywords": [
    "UI",
    "Svelte",
    "UI-Library",
    "element"
  ],
  "author": "koory1st",
  "license": "MIT",
  "homepage": "https://koory1st.github.io/svelte-element-ui"
}
